{
  //enforcing options
  "bitwise": true,
  "camelcase": true,
  "es3": true,
  "freeze": true,
  "immed": true,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "quotmark": true,
  "undef": true,
  "unused": true,
  "strict": true,
  "trailing": true,

  "curly": false,
  "eqeqeq": false,
  "forin": false,
  "indent": false,
  "nonew": false,
  "plusplus": false,

  "maxparams": false,
  "maxdepth": false,
  "maxstatements": false,
  "maxcomplexity": false,
  "maxlen": false,

  //relaxing options
  "eqnull": true,
  "expr": true,

  //environments
  "browser": true,
  "node": true,
  "devel": true,
  "globals": {
    "define": false
  }
}
